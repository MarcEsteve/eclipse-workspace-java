//Especialización de la superclase Vehiculo, es decir, una clase hija
public class Avion extends Vehiculo {
	//Atributos
	private boolean trenAterrizaje;
	//Constructores
	public Avion() {
		super(0.0f , 4 , 0);
		this.trenAterrizaje = true;
	}
	public Avion(float velocidad, boolean trenAterrizaje , int asientos, int orientacionRuedas) {
		super(velocidad , asientos , orientacionRuedas);
		this.trenAterrizaje = false;
	}
	//Métodos
	public void volar() {}
	public void aterrizar() {}
}
