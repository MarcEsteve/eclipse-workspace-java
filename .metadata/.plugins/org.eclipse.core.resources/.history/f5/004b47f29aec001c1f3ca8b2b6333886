package com.app.strings;

public class Ejercicios413 {

	public static void main(String[] args) {
		String a = "abc";
		String b = "aef";
		//Ejercicio: escribe un programa que dado 
		//dos String compruebe si los dos primeros
		//caracteres son iguales.
		if (a.charAt(0)==b.charAt(0) && a.charAt(1)==b.charAt(1)) {
			System.out.println("Son dos caracteres iniciales iguales");
		} else {
			System.out.println("Son dos caracteres iniciales diferentes");
		}
		

	}

}
