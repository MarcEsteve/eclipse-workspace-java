package com.app.strings;

public class Ejercicios413 {

	public static void main(String[] args) {
		String a = "abc";
		String b = "cef";
		//Ejercicio: escribe un programa que dado 
		//dos String compruebe si los dos primeros
		//caracteres son iguales.
		if (a.charAt(0)==b.charAt(0)) {
			System.out.println("Son caracteres iniciales iguales");
		} else {
			System.out.println("Son caracteres iniciales diferentes");
		}
		

	}

}
